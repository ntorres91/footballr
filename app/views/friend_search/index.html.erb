<h2> Friends In NYC </h2>

<table>
  <thead>
    <tr>
      <th>Friend's Username</th>
      <th>Friend's Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><%= link_to "Back", dashboard_path %></br>

