<h2> Games In NYC </h2>

<table>
  <tr>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
  </tr>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.title %></td>
      <td><%= link_to "Show", event %></td>
      <td><%= link_to "Edit", edit_event_path(event) %></td>
      <td><%= link_to "Destroy", event, :confirm => 'Sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<%= link_to "New Game", new_event_path %>
<%= link_to "Back", dashboard_path %>
