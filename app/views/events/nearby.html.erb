<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Address</th>
      <th>Starts</th>
      <th>Ends</th>
      <th>Created By</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.address %></td>
        <td><%= event.starts_at %></td>
        <td><%= event.ends_at %></td>
        <td><%= event.user.user name%></td>
      </tr>
    <% end %>
  </tbody>
</table>
